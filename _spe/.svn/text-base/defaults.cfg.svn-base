#workspaces
globalRecent                = 1
globalFolders               = 1
globalNotes                 = 1
globalFileList              = 0
SaveOnExit                  = 1
SaveWorkspaceOnFileSave     = 1
CloseChildrenOnNewWorkspace = 1
