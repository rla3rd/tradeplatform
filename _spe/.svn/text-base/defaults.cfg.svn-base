CallTips                = whole documentation
Font                    = Courier,default
MaxChildren             = 1
UpdateSidebar           = realtime
WordChars               = _.abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789
